{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "target": "es6",
	"allowJs": true,
    "strict": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"noImplicitAny": true,
	  "moduleResolution": "node",
	  "lib": [
			"es2015",
			"dom"
		],
	  "paths": {
			"~/*": ["./*"],
		  "@/*": ["src/*"]
		},
		"types": [
			"node",
			"jest"
		]
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.vue",
		"test/**/*.ts"
, "src/boot/Multifilters.ts"	],
	"exclude": [
		".quasar",
		"dist",
		"node_modules"
	]

}
